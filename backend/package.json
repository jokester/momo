{
  "name": "hanko",
  "version": "1.0.0",
  "description": "a microservice for user authenticate",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist tsconfig.build.tsbuildinfo",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "DEBUG=hanko:* nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "DEBUG=hanko:* jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typecheck": "tsc --noEmit --incremental false",
    "typecheck:watch": "tsc --watch --noEmit --incremental false",
    "typeorm:dev": "ts-node node_modules/typeorm/cli.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs/common": "^6.11",
    "@nestjs/config": "^0.3.0",
    "@nestjs/core": "^6.11",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/platform-express": "^6.11",
    "@types/nanoid": "^2.1.0",
    "class-transformer": "^0.2.3",
    "debug": "^4.1.1",
    "fp-ts": "^2",
    "google-auth-library": "^5.10.1",
    "googleapis": "^47.0.0",
    "nanoid": "^2.1.11",
    "pg": "^7.18.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14",
    "@nestjs/schematics": "^6.9",
    "@nestjs/testing": "^6.11",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/jest": "^25",
    "@types/node": "^12",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7"
  },
  "resolutions": {
    "@types/node": "^12"
  }
}
